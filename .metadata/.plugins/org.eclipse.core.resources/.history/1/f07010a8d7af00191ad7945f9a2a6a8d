package com.practise.java.lambdaExample;


interface Lambda{
	void show();
}

interface Lambda1{
	int add(int i, int j);
}


public class LambdaExample3 {

	public static void main(String[] args) {
		
		Lambda a = () -> System.out.println("Show from LambdaExample1");

		a.show();
		
		Lambda1 b =  (int i, int j) -> i + j;
	}

}
